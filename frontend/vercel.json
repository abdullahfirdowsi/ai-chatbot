{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.[a-zA-Z0-9]*$)",
      "headers": { 
        "Cache-Control": "public, max-age=31536000, immutable" 
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist/frontend/browser",
  "installCommand": "npm install",
  "framework": "angular"
}
