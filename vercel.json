{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontend/dist/frontend/browser"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.[a-zA-Z0-9]*$)",
      "headers": { 
        "Cache-Control": "public, max-age=31536000, immutable" 
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
